{
  "name": "lulumi-browser",
  "productName": "Lulumi-browser",
  "version": "1.3.0",
  "electronVersion": "1.7.1",
  "description": "Lulumi-browser is a light weight browser coded with vue and electron",
  "main": "./dist/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:darwin": "cross-env BUILD_TARGET=darwin node .electron-vue/build.js",
    "build:linux": "cross-env BUILD_TARGET=linux node .electron-vue/build.js",
    "build:mas": "cross-env BUILD_TARGET=mas node .electron-vue/build.js",
    "build:win32": "cross-env BUILD_TARGET=win32 node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "e2e": "yarn run pack && ava",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src helper test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src helper test",
    "pack": "yarn run pack:main && yarn run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "yarn run unit && yarn run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "yarn run lint:fix"
  },
  "author": "Boik Su <boik@tdohacker.org>",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-runtime": "^6.23.0",
    "electron-localshortcut": "^0.6.1",
    "fetch-jsonp": "^1.0.6",
    "lulumi": "^0.0.5",
    "node-localstorage": "^1.3.0",
    "punycode": "^2.1.0",
    "strip-css-comments": "^3.0.0",
    "vue": "^2.3.3",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^6.1.1",
    "vue-router": "^2.5.3",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel": "^6.23.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.0.11",
    "cfonts": "^1.1.3",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.1",
    "del": "^2.2.2",
    "devtron": "^1.4.0",
    "electron": "^1.7.1",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-packager": "^8.7.0",
    "electron-rebuild": "^1.5.11",
    "element-ui": "^1.3.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^2.0.3",
    "eslint-plugin-import": "^2.2.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "git-rev-sync": "^1.9.1",
    "html-webpack-plugin": "^2.28.0",
    "inject-loader": "^3.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.2.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.3",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "iview": "^2.0.0-rc.13",
    "mocha": "^3.4.1",
    "multispinner": "^0.2.1",
    "pug": "^2.0.0-rc.1",
    "pug-html-loader": "^1.1.4",
    "require-dir": "^0.3.1",
    "sortablejs": "^1.5.1",
    "spectron": "^3.7.0",
    "style-loader": "^0.17.0",
    "tree-kill": "^1.1.0",
    "url-loader": "^0.5.8",
    "vue-awesome": "^2.3.1",
    "vue-focus": "^2.1.0",
    "vue-hot-reload-api": "^2.1.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^12.0.4",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  },
  "ava": {
    "files": [
      "test/e2e/index.js"
    ],
    "concurrency": 5,
    "failWithoutAssertions": false,
    "powerAssert": false,
    "require": [
      "babel-register"
    ]
  }
}
