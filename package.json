{
  "name": "lulumi-browser",
  "productName": "Lulumi-browser",
  "version": "1.0.0",
  "electronVersion": "1.6.4",
  "description": "Lulumi-browser is a light weight browser coded with vue and electron",
  "scripts": {
    "build": "node tasks/release.js",
    "build:clean": "cross-env PLATFORM_TARGET=clean node tasks/release.js",
    "build:darwin": "cross-env PLATFORM_TARGET=darwin node tasks/release.js",
    "build:linux": "cross-env PLATFORM_TARGET=linux node tasks/release.js",
    "build:mas": "cross-env PLATFORM_TARGET=mas node tasks/release.js",
    "build:win32": "cross-env PLATFORM_TARGET=win32 node tasks/release.js",
    "dev": "node tasks/runner.js",
    "e2e": "yarn run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter app test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix app test",
    "pack": "yarn run pack:main && yarn run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack -p --progress --colors --config webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack -p --progress --colors --config webpack.renderer.config.js",
    "test": "yarn run unit && yarn run e2e",
    "unit": "cross-env BABEL_ENV=testing-unit karma start test/unit/karma.conf.js",
    "postinstall": "yarn run lint:fix && cd app && yarn install"
  },
  "author": "Boik Su <boik@tdohacker.org>",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.3",
    "del": "^2.2.2",
    "devtron": "^1.4.0",
    "electron": "^1.6.4",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.1.0",
    "electron-packager": "^8.6.0",
    "electron-rebuild": "^1.5.7",
    "element-ui": "^1.2.5",
    "eslint": "^3.18.0",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-import": "^2.2.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "git-rev-sync": "^1.8.0",
    "html-webpack-plugin": "^2.28.0",
    "inject-loader": "^3.0.0-beta4",
    "json-loader": "^0.5.4",
    "karma": "^1.5.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.30",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.2.0",
    "pug": "^2.0.0-beta11",
    "pug-loader": "^2.3.0",
    "require-dir": "^0.3.1",
    "sortablejs": "^1.5.1",
    "spectron": "^3.6.0",
    "style-loader": "^0.16.0",
    "tree-kill": "^1.1.0",
    "url-loader": "^0.5.8",
    "vue": "^2.2.4",
    "vue-awesome": "^2.2.0",
    "vue-focus": "^2.1.0",
    "vue-hot-reload-api": "^2.0.11",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^11.3.1",
    "vue-style-loader": "^2.0.4",
    "vue-template-compiler": "^2.2.4",
    "webpack": "^2.3.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {}
}
