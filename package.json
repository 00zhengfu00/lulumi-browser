    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.0",
    "launch-editor-middleware": "^2.2.1",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "mocha": "^5.1.1",
    "mockery": "^2.1.0",
    "modern-normalize": "^0.4.0",
    "multispinner": "^0.2.1",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "pug": "^2.0.3",
    "pug-html-loader": "^1.1.5",
    "require-dir": "^1.0.0",
    "sinon": "^5.0.7",
    "sortablejs": "^1.7.0",
