{
  "name": "lulumi-browser",
  "productName": "Lulumi-browser",
  "version": "1.3.3",
  "electronVersion": "1.7.6",
  "description": "Lulumi-browser is a light weight browser coded with vue and electron",
  "main": "./dist/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:darwin": "cross-env BUILD_TARGET=darwin node .electron-vue/build.js",
    "build:linux": "cross-env BUILD_TARGET=linux node .electron-vue/build.js",
    "build:mas": "cross-env BUILD_TARGET=mas node .electron-vue/build.js",
    "build:win32": "cross-env BUILD_TARGET=win32 node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "fmt": "tsfmt -r",
    "lint": "yarn run fmt && tslint -c tslint.json -e 'src/typings/**/*.ts' 'src/**/*.ts'",
    "lint:fix": "yarn run fmt && tslint -c tslint.json -e 'src/typings/**/*.ts' --fix 'src/**/*.ts'",
    "pack": "yarn run pack:main && yarn run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "e2e": "yarn run pack && ava",
    "unit:renderer": "karma start test/unit/renderer.karma.conf.js",
    "unit:about": "karma start test/unit/about.karma.conf.js",
    "unit": "yarn run unit:renderer && yarn run unit:about",
    "test": "yarn run unit && yarn run e2e",
    "postinstall": "yarn run lint:fix"
  },
  "author": "Boik Su <boik@tdohacker.org>",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^0.16.2",
    "electron-localshortcut": "^2.0.2",
    "electron-process-manager": "^0.0.4",
    "fetch-jsonp": "^1.1.1",
    "fuse.js": "^3.0.5",
    "lulumi": "^0.0.21",
    "node-localstorage": "^1.3.0",
    "punycode": "^2.1.0",
    "strip-css-comments": "^3.0.0",
    "vue": "^2.4.2",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^7.1.1",
    "vue-router": "^2.7.0",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chai": "^4.1.1",
    "chalk": "^2.1.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^1.7.6",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-packager": "^8.7.2",
    "electron-rebuild": "^1.6.0",
    "element-ui": "^1.4.2",
    "eslint": "4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "git-rev-sync": "^1.9.1",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.1",
    "iview": "^2.0.0",
    "json-loader": "^0.5.7",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.2.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "mocha": "^3.5.0",
    "mockery": "^2.1.0",
    "multispinner": "^0.2.1",
    "pug": "^2.0.0-rc.3",
    "pug-html-loader": "^1.1.5",
    "require-dir": "^0.3.2",
    "sinon": "^3.1.0",
    "sortablejs": "^1.6.0",
    "spectron": "^3.7.2",
    "style-loader": "^0.18.2",
    "tree-kill": "^1.1.0",
    "tslint": "^5.6.0",
    "tslint-config-airbnb": "^5.2.1",
    "tslint-loader": "^3.5.3",
    "ts-loader": "^2.3.2",
    "typescript": "next",
    "typescript-formatter": "^5.2.0",
    "url-loader": "^0.5.9",
    "vue-awesome": "^2.3.1",
    "vue-class-component": "^5.0.2",
    "vue-focus": "^2.1.0",
    "vue-hot-reload-api": "^2.1.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.4",
    "vue-property-decorator": "^5.2.1",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.2",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0"
  },
  "ava": {
    "files": [
      "test/e2e/index.js"
    ],
    "concurrency": 5,
    "failWithoutAssertions": false,
    "powerAssert": false,
    "require": [
      "babel-register"
    ]
  },
  "engines": {
    "node": ">=8.1.4",
    "npm": ">=5.4.0"
  }
}
